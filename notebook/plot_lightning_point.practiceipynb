    "c = 300000 # km/s\n",
    "lightning_pt = (-250, 100)\n",
    "lls_1 = (-250, 0)\n",
    "lls_2 = (250,0)\n",
    "delta_t = calc_distance(lls_1, lightning_pt) /c - calc_distance(lls_2, lightning_pt) /c"
   ]
  },
  {
