   "source": [
    "def rotation(deg, x, y):\n",
    "    theta = np.deg2rad(deg)\n",
    "    A = np.array(\n",
    "        [\n",
    "            [np.cos(theta), -np.sin(theta)],\n",
    "            [np.sin(theta), np.cos(theta)]\n",
    "        ]\n",
    "    )\n",
    "    rot_x = x * np.cos(theta) - y* np.sin(theta)\n",
    "    rot_y = x * np.sin(theta) + y*np.cos(theta)\n",
    "    return rot_x, rot_y"
   ]
  },
