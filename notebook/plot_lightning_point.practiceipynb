   "outputs": [],
   "source": [
    "def calc_distance(X1, X2):\n",
    "    dist = np.sqrt(\n",
    "        (X1[0] -X2[0])**2 + (X1[1]- X2[1])**2\n",
    "    )\n",
    "    return dist"
   ]
  },
